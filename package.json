  },
  "scripts": {
    "start": "parcel serve ui/index.html --port 3000 --dist-dir build --hmr-port 1234",
    "build": "parcel build ui/index.html --dist-dir build",
    "test": "node ui/scripts/test.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.9.3",
    "@testing-library/react": "^12.1.5",
    "@types/react-syntax-highlighter": "^13.5.2",
    "buffer": "^5.5.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.0",
    "jest-styled-components": "^7.0.8",
    "prettier": "^2.3.0",
    "process": "^0.11.10"
  },
  "jest": {
    "roots": [
      "<rootDir>/ui/src"
    ],
    "collectCoverageFrom": [
      "ui/src/**/*.{js,jsx,ts,tsx}",
      "!ui/src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/ui/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/ui/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/ui/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/ui/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/ui/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/ui/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!coy)/"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
